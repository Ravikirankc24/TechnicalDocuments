                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol, Liberation Sans">◦</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Hepatitis Virus</fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol, Liberation Sans">◦</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>Genetic Disease</fo:block></fo:list-item-body></fo:list-item>
