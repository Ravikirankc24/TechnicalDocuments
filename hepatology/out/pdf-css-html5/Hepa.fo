                            <fo:block-container font-size="2em" line-height="1.1em" space-after="1em" margin-left="0" margin-right="0" space-before="0" page-break-after="avoid" id="ariaid-title5"><fo:block end-indent="0" start-indent="0" role="H1"><fo:marker marker-class-name="chaptertitle"><fo:inline/> | 5 - <!--content(text)--><fo:inline>Reference</fo:inline></fo:marker><fo:inline>Chapter 5. </fo:inline><fo:inline>Reference</fo:inline></fo:block></fo:block-container>
                            <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"><fo:inline color="rgb(51 , 122 , 183)" text-decoration="underline"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.healthline.com/human-body-maps/liver">https://www.healthline.com/human-body-maps/liver</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.healthline.com/human-body-maps/liver"><fo:inline font-style="italic" text-decoration="underline"/></fo:basic-link></fo:inline></fo:block></fo:block-container>
                            </fo:block></fo:block-container>
